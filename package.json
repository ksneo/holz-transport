{
  "name": "holz-transport",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "set NODE_PATH=./src&&react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "set NODE_PATH=./src&&react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "set NODE_PATH=./src&&react-scripts test --env=jsdom",
    "eject": "set NODE_PATH=./src&&react-scripts eject"
  }
}
